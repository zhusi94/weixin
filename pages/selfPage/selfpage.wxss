/* pages/selfpage.wxss */
page {
  height: 100%;
  padding: 0;
  margin: 0;
  overflow: hidden;
}
.self_container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
.fang {
  width: 100%;
  height: 50%;
  cursor: pointer;
  position: relative;
}
.one {
  background-image: linear-gradient( 135deg, #FAD7A1 10%, #E96D71 100%);
}
.one image {
  width: 14rem;
  position: absolute;
  top: 1.3rem;
  left: 1rem;
}
.two {
  background-image: linear-gradient( 135deg, #92FFC0 10%, #002661 100%);
}
.two image {
  width: 14rem;
  position: absolute;
  top: 1.3rem;
  right: 1rem;
}
.muyuText {
  position: absolute;
  top: 50%;
  right: 8%;
  color: #414B55;
  font-size: 1.6rem;
}
.bbsText {
  position: absolute;
  top: 50%;
  left: 8%;
  color: #414B55;
  font-size: 1.6rem;
}